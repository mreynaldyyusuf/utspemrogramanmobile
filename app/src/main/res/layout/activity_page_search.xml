<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pageSearchVM" type="com.reynaldysapplication.app.modules.pagesearch.data.viewmodel.PageSearchVM"/>
    </data>
    <FrameLayout android:id="@+id/framePageSearch"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnlogo"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowlogo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStacklogo"
                    android:layout_height="@dimen/_147pxv" android:layout_width="@dimen/_263pxh">
                    <ImageView android:id="@+id/imageLogo"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_130pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_202pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_logo}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_logo"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/msg_mau_bantu_siapa"
                        android:id="@+id/etSearchBox"
                        android:inputType="text"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@={pageSearchVM.pageSearchModel.etSearchBoxValue}"
                        android:textColorHint="@color/black_900"
                        style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameTombolSearch"
                    android:layout_height="@dimen/_44pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_103pxv"
                    android:layout_width="@dimen/_78pxh" style="@style/groupStyleblue_1">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_29pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_29pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectanglefour"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <LinearLayout android:id="@+id/linearRowrectanglefour"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleFour"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle4}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4"/>
                    <ImageView android:id="@+id/imageRectangleFive"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle5}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5"/>
                    <ImageView android:id="@+id/imageRectangleSix"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle6}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowlingkungan"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLingkungan"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.3"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtLingkungan}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_lingkungan"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBantuanMedis"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtBantuanMedis}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_bantuan_medis"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInfrastruktur"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_weight="0.32"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtInfrastruktur}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_infrastruktur"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearRowrectanglefourOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleFourOne"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle4_155x103}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4_155x103"/>
                    <ImageView android:id="@+id/imageRectangleFiveOne"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle5_155x103}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5_155x103"/>
                    <ImageView android:id="@+id/imageRectangleSixOne"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle6_155x103}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6_155x103"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowbalita"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBalita"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.31"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtBalita}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_balita"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDifabel"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_81pxh"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtDifabel}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_difabel"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtZakat"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_81pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.31"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtZakat}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_zakat"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearRowrectanglefourTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleFourTwo"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle4_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4_1"/>
                    <ImageView android:id="@+id/imageRectangleFiveTwo"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle5_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5_1"/>
                    <ImageView android:id="@+id/imageRectangleSixTwo"
                        android:layout_height="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_103pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle6_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6_1"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowhewan"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHewan"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.18"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pageSearchVM.pageSearchModel.txtHewan}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl_hewan"/>
                    <LinearLayout android:id="@+id/linearRowrumahibadah"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_55pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRumahIbadah"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.53"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pageSearchVM.pageSearchModel.txtRumahIbadah}"
                            style="@style/txtPoppinsmedium13" tools:text="@string/lbl_rumah_ibadah"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPantiAsuhan"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_weight="0.47"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pageSearchVM.pageSearchModel.txtPantiAsuhan}"
                            style="@style/txtPoppinsmedium13" tools:text="@string/lbl_panti_asuhan"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStyleblue_2">
            <LinearLayout android:id="@+id/linearRowvector"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageBxsearchaltTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_99pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_29pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_bxsearchalt2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bxsearchalt2"/>
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_98pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
